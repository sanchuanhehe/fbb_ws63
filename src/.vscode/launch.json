{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "RISC-V via external GDB Server",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "external",
      "gdbTarget": "host.docker.internal:3333",
      "executable": "${workspaceFolder}/output/ws63/acore/ws63-liteos-app/ws63-liteos-app.elf",
      "gdbPath": "${workspaceFolder}/tools/bin/compiler/riscv/cc_riscv32_musl_105/cc_riscv32_musl/bin/riscv32-linux-musl-gdb",
      "device": "RISC-V",
      "interface": "swd",
      "cwd": "${workspaceFolder}",
      "toolchainPrefix": "riscv32-linux-musl",
      "postLaunchCommands": [
        "monitor halt",
        "monitor reset",
        "monitor halt"
      ],
      "postRestartCommands": [
        "-enable-pretty-printing"
      ],
      "postResetCommands": [
        "-enable-pretty-printing"
      ],
      "preResetCommands": [
        "monitor halt"
      ],
      "runToEntryPoint": "main",
      "stopAtEntry": true,
      "showDevDebugOutput": "both",
      "swoConfig": {
        "enabled": false
      },
      "rttConfig": {
        "enabled": false
      }
    }
  ]
}